{
    "schemaVersion": 1,
    "id": "fog-control",
    "version": "${version}",

    "name": "Fog Control Mod",
    "description": "Allows the user to adjust the (client) distance at which fogs render.",
    "authors": [
        "capnkork"
    ],
    "contact": {
        "homepage": "https://capnkork.com/",
        "sources": "https://github.com/capnkork/fog-control"
    },

    "license": "MIT",
    "icon": "assets/fog-control/icon.png",

    "environment": "client",
    "entrypoints": {
        "modmenu": [
            "com.capnkork.fogcontrol.config.ModMenuIntegration"
        ]
    },
    "accessWidener": "fogcontrol.accesswidener",
    "mixins": [
        "fogcontrol.mixins.json"
    ],

    "depends": {
        "fabricloader": ">=0.15.0",
        "minecraft": "~1.20.4",
        "modmenu": ">=9.0.0",
        "cloth-config2": ">=13.0.112",
        "java": ">=17"
    },
    "suggests": {
    }
}
